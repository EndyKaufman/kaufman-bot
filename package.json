{
  "name": "kaufman-bot",
  "version": "1.3.1",
  "description": "A simple bot for Telegram and Skype",
  "repository": "https://github.com/EndyKaufman/kaufman-bot.git",
  "engines": {
    "node": "8.11.x",
    "npm": "5.8.x"
  },
  "license": "MIT",
  "scripts": {
    "start:dev": "nodemon --exec ts-node ./src/bin/app.ts --watch src --ext .ts",
    "start": "npm run build && npm run serve",
    "serve": "node ./dist/bin/app -s",
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "lint:fix": "tslint -c tslint.json 'src/**/*.ts' --fix",
    "clean": "rimraf dist",
    "build": "npm run clean && tsc --pretty",
    "test": "npm run lint && npm run build && mocha --timeout 15000 --require ts-node/register --recursive src/**/*.spec.ts",
    "watch": "npm run build -- --watch",
    "watch:test": "npm run test -- --watch"
  },
  "author": {
    "name": "EndyKaufman",
    "email": "admin@site15.ru"
  },
  "main": "dist/bin/app.js",
  "typings": "dist/bin/app.d.ts",
  "bin": {
    "kaufman-bot": "bin/kaufman-bot"
  },
  "files": [
    "bin",
    "dist"
  ],
  "devDependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/chai": "^4.2.14",
    "@types/cheerio": "^0.22.23",
    "@types/commander": "^2.12.2",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.11",
    "@types/lodash": "^4.14.167",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.19.14",
    "@types/request": "^2.48.5",
    "@types/sinon": "^7.5.2",
    "chai": "^4.2.0",
    "conventional-changelog-cli": "^2.1.1",
    "conventional-commits-detector": "^1.0.3",
    "conventional-recommended-bump": "^6.1.0",
    "json-stringify-safe": "^5.0.1",
    "mocha": "^6.2.3",
    "nodemon": "^2.0.7",
    "rimraf": "^3.0.2",
    "sinon": "^7.5.0",
    "ts-node": "^8.10.2",
    "ts-node-dev": "^1.1.1",
    "tslint": "^5.20.1",
    "typescript": "^3.9.7"
  },
  "dependencies": {
    "apiai": "^4.0.3",
    "bcrypt": "^3.0.8",
    "body-parser": "^1.19.0",
    "botbuilder": "^4.11.0",
    "charset": "^1.0.1",
    "cheerio": "^1.0.0-rc.5",
    "commander": "^7.0.0",
    "console-stamp": "^0.2.9",
    "dialogflow": "^0.12.2",
    "dotenv": "^8.2.0",
    "encoding": "^0.1.13",
    "express": "^4.17.1",
    "forever-monitor": "^1.7.2",
    "googleapis": "^44.0.0",
    "html-to-text": "^5.1.1",
    "iconv-lite": "^0.5.2",
    "jschardet": "^2.2.1",
    "lodash": "^4.17.20",
    "node-telegram-bot-api": "^0.30.0",
    "request": "^2.88.2",
    "rollbar": "^2.19.4",
    "wikijs": "^6.0.1",
    "wtf_wikipedia": "^7.8.1"
  }
}
